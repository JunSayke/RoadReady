<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@drawable/rectangle"
	tools:context=".activity.dealership.MyVehicles_Activity">

	<LinearLayout
		android:id="@+id/my_vehicles"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent">

		<androidx.constraintlayout.widget.ConstraintLayout
			android:id="@+id/dealershipFormHeader"
			android:layout_width="match_parent"
			android:layout_height="100dp"
			android:background="#888484">

			<androidx.constraintlayout.widget.ConstraintLayout
				android:id="@+id/btnOpenSidebar"
				android:layout_width="28dp"
				android:layout_height="28dp"
				android:background="@color/white"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintTop_toTopOf="parent">


				<ImageView
					android:id="@+id/union"
					android:layout_width="5.33dp"
					android:layout_height="9.33dp"
					android:contentDescription="union"
					android:src="@drawable/sb_iv_union"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintTop_toTopOf="parent" />

			</androidx.constraintlayout.widget.ConstraintLayout>

			<ImageView
				android:id="@+id/ivRedCircle"
				android:layout_width="264dp"
				android:layout_height="264dp"
				android:layout_marginStart="250dp"
				android:layout_marginTop="-180dp"
				android:background="@drawable/circle"
				android:backgroundTint="#FE4949"
				android:contentDescription="@string/red_circle"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:ignore="ImageContrastCheck" />

			<TextView
				android:id="@+id/ivHeader"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_horizontal|center"
				android:gravity="center"
				android:text="@string/text_header_my_vehicles"
				android:textSize="20sp"
				android:textStyle="bold"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintTop_toTopOf="parent" />


		</androidx.constraintlayout.widget.ConstraintLayout>

		<TextView
			android:id="@+id/mvTxtTitle"
			android:layout_width="166.4dp"
			android:layout_height="35.4dp"
			android:layout_gravity="center_horizontal|center"
			android:layout_marginTop="37dp"
			android:gravity="center"
			android:text="@string/text_my_vehicle_title"
			android:textSize="24sp"
			android:textStyle="bold" />

		<ScrollView
			android:id="@+id/mvSVVehicleList"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:layout_gravity="center_horizontal"
			android:layout_weight="1">


			<LinearLayout
				android:id="@+id/mvContainerVehicleList"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical">

				<LinearLayout
					android:id="@+id/mvContainerList1"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="horizontal">

					<androidx.constraintlayout.widget.ConstraintLayout
						android:id="@+id/mvContainerVehicle1"
						android:layout_width="148dp"
						android:layout_height="100dp"
						android:layout_marginStart="1dp"
						android:layout_marginTop="0dp">

						<ImageView
							android:id="@+id/mvImageVehicle1"
							android:layout_width="115dp"
							android:layout_height="76dp"
							android:contentDescription="mv_iv_vehicle"
							app:layout_constraintBottom_toBottomOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintStart_toStartOf="parent"
							app:layout_constraintTop_toTopOf="parent"
							app:srcCompat="@drawable/hp_iv_fortuner" />

						<TextView
							android:id="@+id/mvTxtVehicle1"
							android:layout_width="92.2dp"
							android:layout_height="17.2dp"
							android:layout_gravity="center_horizontal|center"
							android:layout_marginBottom="5dp"
							android:gravity="center"
							android:text="Fortuner"
							android:textSize="12sp"
							android:textStyle="bold"
							app:layout_constraintBottom_toBottomOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintStart_toStartOf="parent" />

						<ImageButton
							android:id="@+id/mvBtnDelete1"
							android:layout_width="29dp"
							android:layout_height="31dp"
							android:background="?android:selectableItemBackground"
							android:contentDescription="mv_ib_delete"
							android:src="@drawable/mv_ib_delete"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintTop_toTopOf="parent" />

					</androidx.constraintlayout.widget.ConstraintLayout>

					<androidx.constraintlayout.widget.ConstraintLayout
						android:id="@+id/mvContainerVehicle2"
						android:layout_width="148dp"
						android:layout_height="100dp"
						android:layout_marginStart="1dp"
						android:layout_marginTop="0dp">

						<ImageView
							android:id="@+id/mvImageVehicle2"
							android:layout_width="115dp"
							android:layout_height="76dp"
							android:contentDescription="mv_iv_vehicle"
							app:layout_constraintBottom_toBottomOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintStart_toStartOf="parent"
							app:layout_constraintTop_toTopOf="parent"
							app:srcCompat="@drawable/hp_iv_fortuner" />

						<TextView
							android:id="@+id/mvTxtVehicle2"
							android:layout_width="92.2dp"
							android:layout_height="17.2dp"
							android:layout_gravity="center_horizontal|center"
							android:layout_marginBottom="5dp"
							android:gravity="center"
							android:text="Fortuner"
							android:textSize="12sp"
							android:textStyle="bold"
							app:layout_constraintBottom_toBottomOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintStart_toStartOf="parent" />

						<ImageButton
							android:id="@+id/mvBtnDelete2"
							android:layout_width="29dp"
							android:layout_height="31dp"
							android:background="?android:selectableItemBackground"
							android:contentDescription="mv_ib_delete"
							android:src="@drawable/mv_ib_delete"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintTop_toTopOf="parent" />

					</androidx.constraintlayout.widget.ConstraintLayout>
				</LinearLayout>

				<LinearLayout
					android:id="@+id/mvContainerList2"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="horizontal">

					<androidx.constraintlayout.widget.ConstraintLayout
						android:id="@+id/mvContainerVehicle3"
						android:layout_width="148dp"
						android:layout_height="100dp"
						android:layout_marginStart="1dp"
						android:layout_marginTop="0dp">

						<ImageView
							android:id="@+id/mvImageVehicle3"
							android:layout_width="115dp"
							android:layout_height="76dp"
							android:contentDescription="mv_iv_vehicle"
							app:layout_constraintBottom_toBottomOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintStart_toStartOf="parent"
							app:layout_constraintTop_toTopOf="parent"
							app:srcCompat="@drawable/hp_iv_fortuner" />

						<TextView
							android:id="@+id/mvTxtVehicle3"
							android:layout_width="92.2dp"
							android:layout_height="17.2dp"
							android:layout_gravity="center_horizontal|center"
							android:layout_marginBottom="5dp"
							android:gravity="center"
							android:text="Fortuner"
							android:textSize="12sp"
							android:textStyle="bold"
							app:layout_constraintBottom_toBottomOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintStart_toStartOf="parent" />

						<ImageButton
							android:id="@+id/mvBtnDelete3"
							android:layout_width="29dp"
							android:layout_height="31dp"
							android:background="?android:selectableItemBackground"
							android:contentDescription="mv_ib_delete"
							android:src="@drawable/mv_ib_delete"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintTop_toTopOf="parent" />

					</androidx.constraintlayout.widget.ConstraintLayout>

					<androidx.constraintlayout.widget.ConstraintLayout
						android:id="@+id/mvContainerVehicle4"
						android:layout_width="148dp"
						android:layout_height="100dp"
						android:layout_marginStart="1dp"
						android:layout_marginTop="0dp">

						<ImageView
							android:id="@+id/mvImageVehicle4"
							android:layout_width="115dp"
							android:layout_height="76dp"
							android:contentDescription="mv_iv_vehicle"
							app:layout_constraintBottom_toBottomOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintStart_toStartOf="parent"
							app:layout_constraintTop_toTopOf="parent"
							app:srcCompat="@drawable/hp_iv_fortuner" />

						<TextView
							android:id="@+id/mvTxtVehicle4"
							android:layout_width="92.2dp"
							android:layout_height="17.2dp"
							android:layout_gravity="center_horizontal|center"
							android:layout_marginBottom="5dp"
							android:gravity="center"
							android:text="Fortuner"
							android:textSize="12sp"
							android:textStyle="bold"
							app:layout_constraintBottom_toBottomOf="parent"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintStart_toStartOf="parent" />

						<ImageButton
							android:id="@+id/mvBtnDelete4"
							android:layout_width="29dp"
							android:layout_height="31dp"
							android:background="?android:selectableItemBackground"
							android:contentDescription="mv_ib_delete"
							android:src="@drawable/mv_ib_delete"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintTop_toTopOf="parent" />

					</androidx.constraintlayout.widget.ConstraintLayout>
				</LinearLayout>

			</LinearLayout>

		</ScrollView>

		<androidx.constraintlayout.widget.ConstraintLayout
			android:id="@+id/mvContainerVehicleButtons"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_gravity="center_horizontal"
			android:layout_marginBottom="30dp"
			android:background="@color/white">


			<androidx.constraintlayout.widget.ConstraintLayout
				android:id="@+id/mvContainerAddVehicle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginStart="100dp"
				app:layout_constraintStart_toEndOf="@+id/mvContainerRemoveVehicle"
				app:layout_constraintTop_toTopOf="parent">

				<ImageButton
					android:id="@+id/mvBtnAddVehicle"
					android:layout_width="58dp"
					android:layout_height="58dp"
					android:background="?android:selectableItemBackground"
					android:contentDescription="mv_iv_addvehicle"
					android:src="@drawable/mv_iv_addvehicle"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintTop_toTopOf="parent" />

				<TextView
					android:id="@+id/mvTxtAddVehicle"
					android:layout_width="101.6dp"
					android:layout_height="23.6dp"
					android:gravity="center"
					android:text="@string/text_add_vehicle"
					android:textSize="16sp"
					android:textStyle="bold"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintTop_toBottomOf="@+id/mvBtnAddVehicle" />

			</androidx.constraintlayout.widget.ConstraintLayout>


			<androidx.constraintlayout.widget.ConstraintLayout
				android:id="@+id/mvContainerRemoveVehicle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintTop_toTopOf="parent">

				<ImageButton
					android:id="@+id/mvBtnRemoveVehicle"
					android:layout_width="58dp"
					android:layout_height="58dp"
					android:background="?android:selectableItemBackground"
					android:contentDescription="mv_iv_removevehicle"
					android:src="@drawable/mv_iv_removevehicle"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintTop_toTopOf="parent" />

				<TextView
					android:id="@+id/mvTxtRemoveVehicle"
					android:layout_width="130.6dp"
					android:layout_height="23.6dp"
					android:gravity="center"
					android:text="@string/text_remove_vehicle"
					android:textSize="16sp"
					android:textStyle="bold"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintTop_toBottomOf="@+id/mvBtnRemoveVehicle" />

			</androidx.constraintlayout.widget.ConstraintLayout>

		</androidx.constraintlayout.widget.ConstraintLayout>

	</LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>